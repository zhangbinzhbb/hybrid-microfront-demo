  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=81920 LOWCODE_ENV=development webpack serve --config ./builds/webpack.dev.js",
    "build:prod": "cross-env LOWCODE_ENV=production webpack --config ./builds/webpack.prod.js",
    "build:sso": "cross-env LOWCODE_ENV=production_sso webpack --config ./builds/webpack.prod.js",
    "lint": "eslint --ext .js,.vue src",
    "preview": "node build/index.js --preview",
    "new": "plop",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "log": "conventional-changelog --config ./node_modules/vue-cli-plugin-commitlint/lib/log -i CHANGELOG.md -s -r 0",
